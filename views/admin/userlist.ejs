<table>
    <thead>
    <th>用户名</th>
    <th>密码</th>
    <th>是否管理员</th>
    <th>操作</th>
    </thead>
    <tbody>
    <% users.forEach(function(user){ %>
    <tr>
        <td><%= user.name %></td>
        <td><%= user.password %></td>
        <td><%= user.role==1?'是':'否' %></td>
        <td><a href="#">修改</a>|<a href="#">删除</a></td>
    </tr>
    <% }) %>
    </tbody>
</table>